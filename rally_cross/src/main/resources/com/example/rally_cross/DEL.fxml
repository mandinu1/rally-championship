<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rally_cross.DELController">
   <children>
      <TableView fx:id="tableView" layoutX="-1.0" layoutY="2.0" prefHeight="315.0" prefWidth="600.0" style="-fx-background-color: gray;">
        <columns>
          <TableColumn fx:id="name" prefWidth="223.0" text="NAME" />
          <TableColumn fx:id="age" minWidth="0.0" prefWidth="74.0" text="AGE" />
            <TableColumn fx:id="team" minWidth="0.0" prefWidth="89.0" text="TEAM" />
            <TableColumn fx:id="car" minWidth="0.0" prefWidth="99.0" text="CAR" />
            <TableColumn fx:id="currentpoints" minWidth="0.0" prefWidth="114.0" text="CURRENT POINT" />
        </columns>
      </TableView>
      <Button fx:id="remove" layoutX="465.0" layoutY="338.0" mnemonicParsing="false" onAction="#removeCustomer" prefHeight="39.0" prefWidth="94.0" text="DELETE">
         <font>
            <Font name="Baskerville Old Face" size="14.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="36.0" layoutY="338.0" mnemonicParsing="false" onAction="#navigatorBack" prefHeight="39.0" prefWidth="94.0" text="BACK">
         <font>
            <Font name="Baskerville Old Face" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
